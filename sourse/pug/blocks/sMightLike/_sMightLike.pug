mixin sMightLike()
	// start sMightLike
	+b.SECTION.sMightLike.bg-secondary.section#sMightLike&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 ВАМ МОГУТ ПОНРАВИТСЯ
			.row
				.col-lg-3.col-6
					+cardItem('1')
				.col-lg-3.col-6
					+cardItem('2')
				.col-lg-3.col-6
					+cardItem('1')
				.col-lg-3.col-6
					+cardItem('2')
	// end sMightLike